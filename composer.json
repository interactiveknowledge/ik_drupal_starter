{
  "name": "interactiveknowledge/ik_drupal_starter",
  "type": "project",
  "description": "Interactive Knowledge's base Drupal site installation.",
  "license": "GPL-2.0-or-later",
  "require": {
    "drupal/address": "^1.8",
    "drupal/admin_toolbar": "^3.0",
    "drupal/adminimal_admin_toolbar": "^1.5",
    "drupal/adminimal_theme": "^1.6",
    "drupal/anchor_link": "^2.5",
    "drupal/autocomplete_deluxe": "^2.0@RC",
    "drupal/automated_crop": "2.0.x-dev@dev",
    "drupal/cache_control_override": "^1.0@alpha",
    "drupal/captcha": "^1.0",
    "drupal/chosen": "^3.0",
    "drupal/ckeditor_indentblock": "^1.0@beta",
    "drupal/ckeditor_tabletoolstoolbar": "^2.0@RC",
    "drupal/color_field": "2.x-dev",
    "drupal/config_split": "^1.6",
    "drupal/consumer_image_styles": "^4",
    "drupal/consumers": "^1.11",
    "drupal/crop": "2.x-dev@dev",
    "drupal/date_popup": "^1.1",
    "drupal/decoupled_router": "^2.0",
    "drupal/devel": "^4.0",
    "drupal/diff": "^1.0",
    "drupal/disable_messages": "^2.0",
    "drupal/easy_breadcrumb": "^2.0",
    "drupal/editor_advanced_link": "^1.8",
    "drupal/embed": "^1.4",
    "drupal/entity_block": "^1.0@beta",
    "drupal/entity_embed": "^1.2",
    "drupal/entity_field_condition": "^1.2",
    "drupal/entityqueue": "^1.2",
    "drupal/environment_indicator": "^4.0",
    "drupal/eu_cookie_compliance": "^1.19",
    "drupal/field_group": "^3.1",
    "drupal/geocoder": "^3.12",
    "drupal/geofield": "^1.16",
    "drupal/geolocation": "^3.2",
    "drupal/google_analytics": ">=2.5",
    "drupal/google_tag": "^1.4",
    "drupal/http_response_headers": "^2.0",
    "drupal/ik_constant_contact": "^3.0",
    "drupal/ik_modals": "^3.0",
    "drupal/image_widget_crop": "^2.3",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/jsonapi_extras": "^3.16",
    "drupal/layout_builder_restrictions": "^2.8",
    "drupal/layout_library": "^1.0@beta",
    "drupal/libraries": "^3.0@beta",
    "drupal/linkit": "^6.0",
    "drupal/media_entity_file_replace": "^1.0",
    "drupal/media_library_edit": "^2.0",
    "drupal/media_library_form_element": "^2.0",
    "drupal/metatag": "^1.16",
    "drupal/migrate_devel": "^2.0@alpha",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/migrate_upgrade": "^3.2",
    "drupal/moderation_dashboard": "^2.0@alpha",
    "drupal/moderation_sidebar": "^1.4",
    "drupal/new_relic_rpm": "^1.3",
    "drupal/optional_end_date": "^1.1",
    "drupal/page_manager": "^4.0@beta",
    "drupal/panelizer": "^4.4",
    "drupal/panels": "^4.6",
    "drupal/pantheon_advanced_page_cache": "^1.1",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/permissions_filter": "^1.2",
    "drupal/protected_pages": "^1.3",
    "drupal/purge": "^3.0",
    "drupal/purge_purger_http": "^1.0",
    "drupal/quicklink": "^1.2",
    "drupal/recaptcha": "^3.0",
    "drupal/redirect": "^1.6",
    "drupal/redirect_after_login": "^2.6",
    "drupal/restui": "^1.18",
    "drupal/scanner": "^1.0@RC",
    "drupal/scheduler": "^1.4",
    "drupal/scheduler_content_moderation_integration": "^1.3",
    "drupal/schemata": "^1.0@beta",
    "drupal/search_api": "^1.16",
    "drupal/search_api_autocomplete": "^1.4",
    "drupal/search_api_location": "^1.0@alpha",
    "drupal/search_api_solr": "^4.2",
    "drupal/search_api_sorts": "^1.0@beta",
    "drupal/sendgrid_integration": "^1.7",
    "drupal/simple_oauth": "^5.2",
    "drupal/slick": "^2.4",
    "drupal/slick_entityreference": "^2.0",
    "drupal/smart_trim": "^1.3",
    "drupal/token": "^1.7",
    "drupal/token_filter": "^1.1",
    "drupal/twig_tweak": "^2.4",
    "drupal/ultimate_cron": "^2.0@alpha",
    "drupal/unpublished_nodes_redirect": "^2.0@beta",
    "drupal/varnish_purge": "^2.0",
    "drupal/view_mode_page": "^4.0",
    "drupal/views_bulk_edit": "^2.5",
    "drupal/views_bulk_operations": "^4.1",
    "drupal/views_infinite_scroll": "^1.9",
    "drupal/webform": "^6.0",
    "drupal/webform_migrate": "^1.1",
    "drupal/webform_rest": "^4.0@beta",
    "drupal/xmlsitemap": "^1.0",
    "geocoder-php/google-maps-provider": "^4.6",
    "harvesthq/chosen": "^1.8",
    "recurser/jquery-simple-color": "^1.2",
    "urodoz/truncate-html": "^1.0"
  },
  "require-dev": {
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-recommended": "^9",
    "drush/drush": "^10.0",
    "kint-php/kint": "^3.3"
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "jquery-simple-color": {
      "type": "package",
      "package": {
        "name": "recurser/jquery-simple-color",
        "version": "1.2.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/recurser/jquery-simple-color/archive/v1.2.2.zip",
          "type": "zip"
        },
        "require": {
            "composer/installers": "^1.9"
        }
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/console-extend-plugin": true,
      "drupal/core-composer-scaffold": true
    }
  },
  "scripts": {
    "remove-git-submodules" : "find . -mindepth 2 -type d -name .git | xargs rm -rf",
    "reset-custom-robots": "cp base/custom-scaffold/robots.txt web/robots.txt",
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "@remove-git-submodules",
      "@reset-custom-robots"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "@remove-git-submodules",
      "@reset-custom-robots"
    ]
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "excludes": [
          ".htaccess",
          "robots.txt"
      ]
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
    },
    "enable-patching": true,
    "patches": {
      "drupal/address": {
        "Add support for feeds target": "https://www.drupal.org/files/issues/2019-04-19/address-feeds-address-target-2882589-23.patch"
      },
      "drupal/add_to_head": {
        "Invalid key in render array": "https://www.drupal.org/files/issues/2021-09-01/add_to_head-footer_bad_render_array.patch"
      },
      "drupal/anchor_link": {
        "Add linkit support to url": "https://www.drupal.org/files/issues/2020-08-04/anchor_link-linkit-support_2958532_0.patch"
      },
      "drupal/autocomplete_deluxe": {
        "Patch term labels so terms with same name don't throw error": "patches/autocomplete_deluxe-same_term_name_error.patch"
      },
      "drupal/core": {
        "Layout builder issue with invalid uuid": "https://www.drupal.org/files/issues/2021-12-09/3008924-rerolled-24.patch",
        "Views patch to allow entity references for search_api": "https://www.drupal.org/files/issues/2021-12-02/2429699-453-9.3.x.patch",
        "Fix getColumns on boolean error": "https://www.drupal.org/files/issues/2020-10-29/2893747-33.patch",
        "Fix for isTranslatable() on null error": "https://www.drupal.org/files/issues/2019-12-05/2928108-49.patch",
        "Media library edit button": "https://www.drupal.org/files/issues/2021-11-03/drupal-2985168-903-106-media-widget-edit-button.patch",
        "Patch for fixing autocomplete terms with parenthesis https://www.drupal.org/project/drupal/issues/2924783#comment-14061776": "https://www.drupal.org/files/issues/2021-04-15/2924783-13.patch",
        "Media edit from wysiwyg https://www.drupal.org/project/drupal/issues/3168868#comment-13954462": "https://www.drupal.org/files/issues/2021-01-04/3168868-9.patch",
        "Add actions for VBO for moderation changes https://www.drupal.org/project/drupal/issues/2797583": "https://www.drupal.org/files/issues/2020-12-19/2797583-181.patch",
        "Add allow null": "https://www.drupal.org/files/issues/2021-12-09/2769407-51.patch",
        "GD toolkit available memory": "https://www.drupal.org/files/issues/2020-11-08/2583041-84.patch",
        "Quickedit getPluginId on null error": "patches/core-quickedit_getPluginId-on-null.patch",
        "Disable 404 and 403 logging": "patches/core_disable-404-and-403-errors.patch",
        "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch"
      },
      "drupal/config_split": {
        "Fix array_merge error": "patches/config_split-array_merge_error.patch"
      },
      "drupal/date_popup": {
        "Undefined index type error": "https://www.drupal.org/files/issues/2020-09-16/date_popup-attribute_is_missing-3042555-16-8x1_1.patch"
      },
      "drupal/entity_field_condition": {
        "Negate option fails patch": "https://www.drupal.org/files/issues/2020-02-03/entity_field_condition-negate_option_fails-3110833-2.patch"
      },
      "drupal/helper": {
        "Patch entitybrowsertrait, https://www.drupal.org/project/entity_browser/issues/2816881#comment-13472529": "patches/helper-patch_for_entitybrowserformtrait.patch"
      },
      "drupal/image_widget_crop": {
        "Adjust cropbox when crop is applied": "https://www.drupal.org/files/issues/2021-06-09/crop_widget_not_adjusting_to_existing_crop-3214365-9.patch"
      },
      "drupal/layout_library": {
        "Add hide preview checkbox": "https://www.drupal.org/files/issues/2019-11-07/3081497_add-hide-content-preview-02.patch",
        "Recursion error fix": "patches/layout_library-recursive-rendering.patch"
      },
      "drupal/panels": {
        "2878684 - Use String.match to correlate regions when switching Layouts in Panels IPE": "https://www.drupal.org/files/issues/panels-ipe-2878684-3.patch"
      },
      "drupal/panelizer": {
        "2778565 - Multilingual support for Panelizer": "https://www.drupal.org/files/issues/2020-03-23/2778565-47.patch"
      },
      "drupal/protected_pages": {
        "Allow wildcard paths": "https://www.drupal.org/files/issues/2020-05-19/2890122-24.patch"
      },
      "drupal/purge_purger_http": {
        "Patch problem with HTTPS requests": "https://www.drupal.org/files/issues/2018-11-29/2892552-verify-option-scheme-update-6.patch"
      },
      "drupal/redirect_after_login": {
        "Headers have already been sent after upgrade to Drupal 9.2": "https://www.drupal.org/files/issues/2021-06-20/3214949.patch"
      },
      "drupal/search_api": {
        "Allow config in settings.php to override backend configurations": "patches/search_api-allow-config-to-override.patch",
        "Add support for entity reference fields multiple": "https://www.drupal.org/files/issues/2020-04-02/search_api-2858061-17.patch",
        "Patch for the above to follow it's extension": "patches/search_api-entity-reference.patch"
      },
      "drupal/search_api_location": {
        "Fix for undefined method getLatitude": "https://www.drupal.org/files/issues/2021-09-28/search_api_location_geocoder_undefined_method.patch"
      },
      "drupal/video_embed_field": {
        "Vimeo provider patch for thumbnail error": "patches/video_embed_field-2.2-fix_for_vimeo_thumbnail.patch",
        "Add media form field": "https://www.drupal.org/files/issues/2020-04-15/vef-3039873-media-library-add-24.patch"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

{
  "name": "interactiveknowledge/ik_drupal_starter",
  "type": "drupal-profile",
  "description": "Interactive Knowledge's base Drupal site installation.",
  "license": "GPL-2.0-or-later",
  "version": "1.0.21",
  "repositories": {
    "drupal": {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
    },
    "assets": {
        "type": "composer",
        "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "drupal/address": "^1.8",
    "drupal/admin_toolbar": "^3.0",
    "drupal/adminimal_admin_toolbar": "^1.11",
    "drupal/adminimal_theme": "^1.6",
    "drupal/anchor_link": "^2.5",
    "drupal/autocomplete_deluxe": "^2.0@RC",
    "drupal/backup_migrate": "^5.0@RC",
    "drupal/cache_control_override": "^1.0@alpha",
    "drupal/captcha": "^1.0",
    "drupal/chosen": "^3.0",
    "drupal/ckeditor_indentblock": "^1.0@beta",
    "drupal/color_field": "2.x-dev",
    "drupal/config_split": "^1.6",
    "drupal/consumer_image_styles": "^4",
    "drupal/core": "^9.0",
    "drupal/crop": "^2.1",
    "drupal/date_popup": "^1.1",
    "drupal/decoupled_router": "^1.0",
    "drupal/devel": "^4.0",
    "drupal/diff": "^1.0",
    "drupal/disable_messages": "^2.0",
    "drupal/easy_breadcrumb": "^1.13",
    "drupal/editor_advanced_link": "^1.8",
    "drupal/embed": "^1.4",
    "drupal/entity_block": "^1.0@beta",
    "drupal/entity_embed": "^1.1",
    "drupal/entity_field_condition": "^1.2",
    "drupal/entityqueue": "^1.1",
    "drupal/environment_indicator": "^4.0",
    "drupal/eu_cookie_compliance": "^1.14",
    "drupal/field_group": "^3.1",
    "drupal/geocoder": "^3.12",
    "drupal/geofield": "^1.16",
    "drupal/geolocation": "^3.2",
    "drupal/google_analytics": "^2.5",
    "drupal/google_tag": "^1.4",
    "drupal/http_response_headers": "^2.0",
    "drupal/ik_constant_contact": "^2.0",
    "drupal/ik_modals": "^3.0",
    "drupal/image_widget_crop": "^2.3",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/jsonapi_extras": "^3.16",
    "drupal/layout_builder_restrictions": "^2.8",
    "drupal/libraries": "^3.0@beta",
    "drupal/linkit": "^6.0",
    "drupal/media_library_edit": "^2.0",
    "drupal/media_library_form_element": "^2.0",
    "drupal/metatag": "^1.13",
    "drupal/migrate_devel": "^2.0@alpha",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/migrate_upgrade": "^3.2",
    "drupal/moderation_dashboard": "^1.0@beta",
    "drupal/moderation_sidebar": "^1.4",
    "drupal/new_relic_rpm": "^1.3",
    "drupal/optional_end_date": "^1.1",
    "drupal/pantheon_advanced_page_cache": "^1.1",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/permissions_filter": "^1.2",
    "drupal/protected_pages": "^1.3",
    "drupal/purge": "^3.0",
    "drupal/purge_purger_http": "^1.0",
    "drupal/quicklink": "^1.2",
    "drupal/recaptcha": "^3.0",
    "drupal/redirect": "^1.6",
    "drupal/redirect_after_login": "^2.6",
    "drupal/restui": "^1.18",
    "drupal/scanner": "^1.0@RC",
    "drupal/search_api": "^1.16",
    "drupal/search_api_autocomplete": "^1.4",
    "drupal/search_api_location": "^1.0@alpha",
    "drupal/search_api_solr": "^4.1",
    "drupal/search_api_sorts": "^1.0",
    "drupal/sendgrid_integration": "^1.7",
    "drupal/smart_trim": "^1.3",
    "drupal/token": "^1.7",
    "drupal/token_filter": "^1.1",
    "drupal/twig_tweak": "^2.4",
    "drupal/unpublished_nodes_redirect": "^2.0@beta",
    "drupal/upgrade_status": "^2.0",
    "drupal/varnish_purge": "^2.0",
    "drupal/view_mode_page": "^4.0",
    "drupal/views_bulk_edit": "^2.5",
    "drupal/views_bulk_operations": "^3.9",
    "drupal/webform": "^6.0",
    "drupal/webform_rest": "^4.0@beta",
    "drupal/xmlsitemap": "^1.0",
    "harvesthq/chosen": "^1.8"
  },
  "require-dev": {
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.6",
    "drupal/core-composer-scaffold": "*",
    "drupal/core-dev": "*",
    "drush/drush": "^10.0",
    "kint-php/kint": "^3.3"
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/composer/{$name}": ["type:drupal-module"],
      "web/profiles/composer/{$name}": ["type:drupal-profile"],
      "web/themes/composer/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
    },
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "drush": {
      "services": {
          "drush.services.yml": "^9"
      }
    },
    "patches": {
      "drupal/address": {
        "Add support for feeds target": "https://www.drupal.org/files/issues/2019-04-19/address-feeds-address-target-2882589-23.patch"
      },
      "drupal/anchor_link": {
        "Add linkit support to url": "https://www.drupal.org/files/issues/2020-08-04/anchor_link-linkit-support_2958532_0.patch"
      },
      "drupal/automated_crop": {
        "Patch for reversed aspect ratio": "https://www.drupal.org/files/issues/2018-12-09/3017217-5.patch",
        "Drupal 9 compatibility patches": "https://www.drupal.org/files/issues/2020-04-09/deprecated-3123489-3.patch"
      },
      "drupal/core": {
          "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
          "Don't validate subrequest responses": "web/profiles/composer/ik_starter/patches/jsonapi-8.x-2.x-turn-off-subrequest-validation.patch"
      },
      "drupal/crop": {
        "Add automatic crop integration": "https://www.drupal.org/files/issues/2018-07-14/2830768-crop-automated_crop_integration-24.patch"
      },
      "drupal/config_split": {
        "Fix array_merge error": "web/profiles/composer/ik_starter/patches/config_split-array_merge_error.patch"
      },
      "drupal/date_popup": {
        "Undefined index type error": "https://www.drupal.org/files/issues/2020-09-16/date_popup-attribute_is_missing-3042555-16-8x1_1.patch"
      },
      "drupal/helper": {
        "Patch entitybrowsertrait": "web/profiles/composer/ik_starter/patches/helper-patch_for_entitybrowserformtrait.patch"
      },
      "drupal/layout_library": {
        "Fix migration issue for fromEntity Error": "web/profiles/composer/ik_starter/patches/layout_library-Error-on-migration-with-fromEntity.patch",
        "Add hide preview checkbox": "https://www.drupal.org/files/issues/2019-11-07/3081497_add-hide-content-preview-02.patch",
        "Recursion error fix": "web/profiles/composer/ik_starter/patches/layout_library-recursive-rendering.patch"
      },
      "drupal/purge_purger_http": {
        "Patch problem with HTTPS requests": "https://www.drupal.org/files/issues/2018-11-29/2892552-verify-option-scheme-update-6.patch"
      },
      "drupal/search_api": {
        "Allow config in settings.php to override backend configurations": "web/profiles/composer/ik_starter/patches/search_api-allow-config-to-override.patch",
        "Add support for entity reference fields multiple": "https://www.drupal.org/files/issues/2020-04-02/search_api-2858061-17.patch",
        "Patch for the above to follow it's extension": "web/profiles/composer/ik_starter/patches/search_api-entity-reference.patch"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
